{
  "name": "bardboardjs",
  "version": "2.3.0",
  "main": "server/app.js",
  "scripts": {
    "setup:env": "node scripts/setup-env.js",
    "dev": "concurrently -k \"npm:dev:bot\" \"npm:dev:web\"",
    "dev:bot": "node server/app.js",
    "dev:web": "next dev",
    "build": "next build",
    "start": "concurrently -k \"npm:start:bot\" \"npm:start:web\"",
    "start:bot": "node server/app.js",
    "start:web": "next start",
    "lint": "eslint .",
    "test": "node --test"
  },
  "keywords": [
    "nodejs",
    "dnd",
    "dungeons",
    "dragons",
    "dungeon",
    "dragon",
    "soundboard",
    "discord",
    "bot",
    "discordjs"
  ],
  "author": "github.com/giabb",
  "license": "GPL-3.0-or-later",
  "description": "Soundboard for DND sessions (or whatever you like)",
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.9",
    "cors": "^2.8.6",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "helmet": "^8.1.0",
    "libsodium-wrappers": "^0.8.2",
    "multer": "^2.0.2",
    "music-metadata": "^11.12.0",
    "next": "^16.1.6",
    "opusscript": "^0.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "session-file-store": "^1.5.0"
  },
  "overrides": {
    "tar": "^7.5.7",
    "undici": "^7.21.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6"
  }
}

