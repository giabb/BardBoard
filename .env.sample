DISCORD_TOKEN='PasteYourDiscordBotTokenHere'
CHANNEL_ID='PasteYourDiscordChannelIdHere'
BOT_PORT=3000
# Auth (optional). If set, all requests require auth. I suggest to set up a 32+ chars random string as SESSION_SECRET.
AUTH_USER=
AUTH_PASS=
SESSION_SECRET=
LOGIN_REMEMBER_DAYS=30
SESSION_DIR=./sessions
# Additional security settings
RATE_LIMIT_AUDIO=120
RATE_LIMIT_FILES=60
UPLOAD_MAX_MB=50
# Category folder name used for overlay noises (default !noises)
NOISES_FOLDER=!noises
# Optional CORS policy if UI and API are on different hosts (otherwise leave it blank)
CORS_ORIGINS=
